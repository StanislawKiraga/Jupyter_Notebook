   "source": [
    "def generator(n):\n",
    "    for i in range(1, n + 1):\n",
    "        yield i\n",
    "\n",
    "number_sum = 0\n",
    "for number in generator(23):\n",
    "    number_sum = number_sum + number\n",
    "print(number_sum)"
   ]
  }
